ANDROID_HOME=$HOME/Android/Sdk; export ANDROID_HOME

path=(
  $path
  "$ANDROID_HOME/emulator"
  "$ANDROID_HOME/tools"
  "$ANDROID_HOME/tools/bin"
  "$ANDROID_HOME/platform-tools"
)

alias occ='ssh -t home.lan docker exec -it --user www-data nextcloud ./occ'
alias photoprism='ssh -t home.lan docker exec -it photoprism photoprism'
alias stream='summon -f ~/Development/MyStuff/servers/secrets.yml -- ~/bin/stream.sh'
